<?xml version="1.0"?>
<launch>
<env name="GAZEBO_MODEL_PATH"
       value="$(find ur5_gazebo)/models:$(optenv GAZEBO_MODEL_PATH)" />
  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="paused"     default="true"/>
    <arg name="world_name" value="$(find ur5_gazebo)/worlds/final.world"/>
    <!-- more default parameters can be changed here -->
  </include>

  <!-- send robot urdf to param server -->
  <include file="$(find arm)/launch/ur5_upload.launch" />

  <!-- load the controllers -->
  <rosparam file="$(find arm)/controller/joint_state_controller.yaml" command="load"/> 
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" 
        output="screen" args="robotiq_85_left_knuckle_joint_position_controller shoulder_pan_joint_position_controller shoulder_lift_joint_position_controller elbow_joint_position_controller wrist_1_joint_position_controller wrist_2_joint_position_controller wrist_3_joint_position_controller joint_state_controller"/> 


  <!-- push robot_description to factory and spawn robot in gazebo -->
  <node name="spawn_gazebo_model" pkg="gazebo_ros" type="spawn_model" args="-urdf -param robot_description -model robot -z 0.900 -J shoulder_pan_joint 0 -J shoulder_lift_joint -1.58 -J elbow_joint 1.58 -J wrist_1_joint -1.58 -J wrist_2_joint -1.58 -J wrist_3_joint -1.58" output="screen"/>
  
  <include file="$(find arm)/launch/controller_utils.launch"/>

  <!-- start this controller --> 
  <rosparam file="$(find arm)/controller/arm_controller_ur5.yaml" command="load"/>

  <node name="arm_controller_spawner" pkg="controller_manager" type="controller_manager" args="arm_controller shoulder_pan_joint_position_controller shoulder_lift_joint_position_controller elbow_joint_position_controller wrist_1_joint_position_controller wrist_2_joint_position_controller wrist_3_joint_position_controller joint_state_controller" respawn="false" output="screen"/>
  <!-- load other controllers 

  <node name="ros_control_controller_manager" pkg="controller_manager" type="controller_manager" respawn="false" output="screen" args="load joint_group_position_controller" /> -->

  <!-- delete old bricks -->
  <node pkg="ur5_gazebo" name="delete" type="spawner_2.py" output="screen" launch-prefix="bash -c 'sleep 5.0; $0 $@' "/>

</launch>
